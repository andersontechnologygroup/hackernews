{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "HackerNewsApi": {
    "BaseUrl": "https://hacker-news.firebaseio.com/v0/"
  },
  "HackerNewsServiceSettings": {
    "CacheKey": "HackerNewsStoryCacheKey",
    "CacheTimeoutInMinutes": 5,
    "NewStoriesJSONPath": "newstories.json",
    "ItemJSONPath": "item/{storyId}.json",
    "NumberOfStoriesToPull": 200
  },
  "Jwt": {
    "Key": "ThisIsMySuperSecretKeyForHackerNewsApi12345",
    "Issuer": "https://nextech.com/jasonanderson/hackernews",
    "Audience": "https://nextech.com/jasonanderson/hackernews"
  },

  "Kestrel": {
    "Endpoints": {
      "HttpsFromPem": {
        "Url": "https://hackernews.anderson-technology-group.com:3000",
        "Certificate": {
          "Path": "/etc/letsencrypt/live/hackernews.anderson-technology-group.com/fullchain.pem",
          "KeyPath": "/etc/letsencrypt/live/hackernews.anderson-technology-group.com/privkey.pem"
        }
      }
    }
  }



}
